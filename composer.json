{
  "name": "mcc-api-tools/request-object-bundle",
  "description": "Simple RequestObject",
  "minimum-stability": "stable",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Elias Bulychov",
      "email": "iliabulychov+git@gmail.com"
    }
  ],
  "require": {
    "php": ">=8.1",
    "ext-json": "*",
    "symfony/config": "^6.2|7.0",
    "symfony/dependency-injection": "^6.2|^7.0",
    "symfony/http-foundation": "^6.2|^7.0",
    "symfony/http-kernel": "^6.2|^7.0",
    "symfony/property-access": "^6.2|^7.0",
    "symfony/serializer": "^6.2|^7.0",
    "symfony/validator": "^6.2|^7.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.0",
    "rector/rector": "^1.1",
    "roave/security-advisories": "dev-latest"
  },
  "config": {
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "MccApiTools\\RequestObjectBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "MccApiTools\\RequestObjectBundle\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/phpunit"
  },
  "extra": {
    "symfony": {
      "allow-contrib": false
    }
  }
}
